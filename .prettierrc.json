{
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "tabWidth": 4,
        "singleQuote": true,
        "bracketSpacing": true,
        "vueIndentScriptAndStyle": false,
        "printWidth": 160
      }
    },
    {
      "files": [
        "*.html",
        "*.js"
      ],
      "options": {
        "tabWidth": 4,
        "singleQuote": true,
        "bracketSpacing": true,
        "printWidth": 160
      }
    }
  ]
}
